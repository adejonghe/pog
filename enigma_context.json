{
  "@context": {
    "sf": "http://www.opengis.net/sf#",
    "geo": "http://www.opengis.net/ont/geosparql#",
    "caprads": "http://idlab.ugent.be/caprads/vocab#" ,
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dwt": {
      "@id": "caprads:deadweight",
      "@type": "xsd:int"
    },
    "gt_shiptype": {
      "@id": "caprads:gt_shiptype",
      "@type": "xsd:int"
    },
    "flag": "caprads:flag",
    "heading": {
      "@id": "caprads:heading",
      "@type": "xsd:int"
    },
    "length": {
      "@id": "caprads:length",
      "@type": "xsd:int"
    },
    "destination": "caprads:destination",
    "lon": {
      "@id": "http://schema.org/longitude",
      "@type": "xsd:float"
    },
    "shiptype": "caprads:shiptype",
    "speed": {
      "@id": "caprads:speed",
      "@type": "xsd:float"
    },
    "elapsed": {
      "@id": "caprads:elapsed",
      "@type": "xsd:int"
    },
    "w_left": {
        "@id": "caprads:wLeft",
        "@type": "xsd:int"
    },
    "rot": {
       "@id": "caprads:rateOfTurn",
       "@type": "xsd:int"
    },
    "width": {
       "@id": "caprads:width",
       "@type": "xsd:int"
    },
    "l_fore": {
        "@id": "caprads:lFore",
        "@type": "xsd:int"
    },
    "course": {
        "@id": "caprads:course",
        "@type": "xsd:int"
    },
    "ship_id": {
        "@id": "caprads:shipId",
        "@type": "xsd:int"
    },
    "shipname": "caprads:shipName",
    "lat": {
      "@id": "http://schema.org/latitude",
      "@type": "xsd:float"
    },
    "timemillis": {
      "@id": "caprads:timemillis",
      "@type": "xsd:long"
    },
    "timestamp": {
      "@id": "caprads:timestamp",
      "@type": "xsd:dateTime"
    },
    "geometry": "geo:hasGeometry",
    "asWKT": "geo:asWKT"
  }
}
